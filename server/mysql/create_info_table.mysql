CREATE TABLE `test`.`beestation` ( `id` INT NOT NULL AUTO_INCREMENT PRIMARY KEY COMMENT 'unique ID', `event` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'Event Date and Time', `sensor` VARCHAR( 30 ) NOT NULL COMMENT 'Unique ID of the sensor', `message` VARCHAR( 127 ) NOT NULL COMMENT 'What happened', INDEX ( `event` , `sensor` ) ) 

